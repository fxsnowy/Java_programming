package JavaCollections;

public class ArrayStructures {

	private int[] theArray = new int[50];
	
	private int arraySize = 10;
	
	public void generateRandomArray(){
		for(int i = 0; i < arraySize; i++){
			
			theArray[i] = (int)(Math.random()*10);
		}
	}
	
